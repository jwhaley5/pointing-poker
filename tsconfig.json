{
	"compilerOptions": {
		"target": "ES2020",
		"module": "ES2022",
		"moduleResolution": "Bundler",
		"strict": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true,
		"types": [
			"aws-lambda"
		]
	},
	"exclude": [
		"frontend/**",
		"node_modules"
	]
}
